CC = gcc
FLAGS = -std=c99 -Wall -s -O3

all: font-converter transpose-level

font-converter: font-converter.c
	$(CC) $(FLAGS) font-converter.c -o font-converter.exe

transpose-level: transpose-level.c
	$(CC) $(FLAGS) transpose-level.c -o transpose-level.exe

clean:
	@rm -rf *.o font-converter.exe transpose-level.exe
